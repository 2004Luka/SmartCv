{
  "builds": [
    { 
      "src": "smartcv-ai/package.json", 
      "use": "@vercel/static-build", 
      "config": { 
        "distDir": "smartcv-ai/dist",
        "buildCommand": "cd smartcv-ai && npm install && npm run build"
      } 
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "smartcv-ai/dist/index.html" }
  ]
} 